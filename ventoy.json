{
    "control": [
        { "VTOY_DEFAULT_SEARCH_ROOT": "/ISO" },
        { "VTOY_MENU_TIMEOUT": "60" },
        { "VTOY_DEFAULT_IMAGE": "/ISO/xubuntu-20.04-desktop-amd64.iso" }
    ],

    "theme": {
        "file": "/ventoy/theme/vimix/theme.txt",
        "gfxmode": "1024x768",
        "display_mode": "GUI",
        "fonts": [
            "/ventoy/theme/vimix/terminus-12.pf2",
            "/ventoy/theme/vimix/terminus-14.pf2",
            "/ventoy/theme/vimix/terminus-16.pf2",
            "/ventoy/theme/vimix/terminus-18.pf2",
            "/ventoy/theme/vimix/dejavu_sans_12.pf2",
            "/ventoy/theme/vimix/dejavu_sans_14.pf2",
            "/ventoy/theme/vimix/dejavu_sans_16.pf2",
            "/ventoy/theme/vimix/dejavu_sans_24.pf2",
            "/ventoy/theme/vimix/dejavu_sans_48.pf2",
            "/ventoy/theme/vimix/dejavu_32.pf2"
        ]
        },

    "persistence": [
        {
            "image": "/ISO/ubuntu-20.04-desktop-amd64.iso",
            "backend": [
                "/persistence/ubuntu_20.04.img"
            ]
        },
        {
            "image": "/ISO/xubuntu-20.04-desktop-amd64.iso",
            "backend": [
                "/persistence/ubuntu_20.04.img"
            ]
        }
    ],



    "menu_alias" : [
        {
            "image": "/ISO/xubuntu-20.04-desktop-amd64.iso",
