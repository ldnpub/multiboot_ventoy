            "alias": "Ubuntu 20.04"
        },
        {
            "image": "/ISO/archlinux-2020.05.01-x86_64.iso",
            "alias": "Archlinux"
        },
        {
            "image": "/ISO/arcolinux-v20.3.4.iso",
            "alias": "Arcolinux"
        },
        {
            "image": "/ISO/boot-repair-disk-64bit.iso",
            "alias": "Boot Repair"
        },
        {
            "image": "/ISO/kali-linux-2020.1b-live-amd64.iso",
            "alias": "Kali"
        },
        {
            "image": "/ISO/kubuntu-20.04-desktop-amd64.iso",
            "alias": "Kubuntu 20.04"
